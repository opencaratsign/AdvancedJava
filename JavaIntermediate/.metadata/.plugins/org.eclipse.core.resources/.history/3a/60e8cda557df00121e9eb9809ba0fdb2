
public class AudioPlayer {

	/**
	 * @param args
	 */
	public static void main(String[] args)  {
		Runtime r = Runtime.getRuntime();
		Process p = null;
		String cmd[] = {"ls"};

		try {
		    p = r.exec(cmd);
		    System.out.println("testing dogs");
		} catch (Exception e) {
		    e.printStackTrace();
		}
	}

}
